{
  "compilerOptions": {
      "target": "es2022",
      "module": "es2022",
      "outDir": "./build/",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": false,
      "skipLibCheck": true,
      "moduleResolution": "node",
  },
  "include": ["src/**/*"],
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  }
}